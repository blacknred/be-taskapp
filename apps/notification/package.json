{
  "name": "@taskapp/notification",
  "private": true,
  "version": "0.0.0",
  "description": "Taskapp notification microservice",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@taskapp/microservice": "workspace:*",
    "@hapi/joi": "^17.1.1",
    "@nest-modules/mailer": "^1.3.22",
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.0.1",
    "@nestjs/core": "^8.0.6",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^8.0.6",
    "@nestjs/platform-express": "^8.0.6",
    "amqp-connection-manager": "^3.7.0",
    "amqplib": "^0.8.0",
    "connect-redis": "^6.0.0",
    "nodemailer": "^6.6.5",
    "redis": "^3.1.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.3.0",
    "twilio": "^3.71.2",
    "web-push": "^3.4.5"
  },
  "devDependencies": {
    "@taskapp/types": "workspace:*",
    "@nestjs/cli": "^8.1.1",
    "@nestjs/schematics": "^8.0.2",
    "@nestjs/testing": "^8.0.6",
    "@types/connect-redis": "^0.0.17",
    "@types/express": "^4.17.13",
    "@types/hapi__joi": "^17.1.7",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.1",
    "@types/nodemailer": "^6.4.4",
    "@types/redis": "^2.8.32",
    "@types/web-push": "^3.3.2",
    "@types/supertest": "^2.0.11",
    "supertest": "^6.1.3"
  }
}
