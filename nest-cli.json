{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/workspace-svc/src",
  "monorepo": true,
  "root": "apps/workspace-svc",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/workspace-svc/tsconfig.app.json"
  },
  "projects": {
    "issue-command-svc": {
      "type": "application",
      "root": "apps/issue-command-svc",
      "entryFile": "main",
      "sourceRoot": "apps/issue-command-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/issue-command-svc/tsconfig.app.json"
      }
    },
    "issue-query-svc": {
      "type": "application",
      "root": "apps/issue-query-svc",
      "entryFile": "main",
      "sourceRoot": "apps/issue-query-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/issue-query-svc/tsconfig.app.json"
      }
    },
    "notification-svc": {
      "type": "application",
      "root": "apps/notification-svc",
      "entryFile": "main",
      "sourceRoot": "apps/notification-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/notification-svc/tsconfig.app.json"
      }
    },
    "search-svc": {
      "type": "application",
      "root": "apps/search-svc",
      "entryFile": "main",
      "sourceRoot": "apps/search-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/search-svc/tsconfig.app.json"
      }
    },
    "report-svc": {
      "type": "application",
      "root": "apps/report-svc",
      "entryFile": "main",
      "sourceRoot": "apps/report-svc/src",
      "compilerOptions": {
        "tsConfigPath": "apps/report-svc/tsconfig.app.json"
      }
    },
    "shared": {
      "type": "library",
      "root": "libs/shared",
      "entryFile": "index",
      "sourceRoot": "libs/shared/src",
      "compilerOptions": {
        "tsConfigPath": "libs/shared/tsconfig.lib.json"
      }
    },
    "core": {
      "type": "library",
      "root": "libs/core",
      "entryFile": "index",
      "sourceRoot": "libs/core/src",
      "compilerOptions": {
        "tsConfigPath": "libs/core/tsconfig.lib.json"
      }
    },
    "eventstore": {
      "type": "library",
      "root": "libs/eventstore",
      "entryFile": "index",
      "sourceRoot": "libs/eventstore/src",
      "compilerOptions": {
        "tsConfigPath": "libs/eventstore/tsconfig.lib.json"
      }
    }
  }
}
