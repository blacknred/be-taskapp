{
  "name": "taskapp",
  "private": true,
  "license": "MIT",
  "version": "0.0.1",
  "description": "Sample application for managing tasks",
  "author": "Dmitry Tretyakov <tretyakov.dmi@gmail.com> (https://blacknred.github.io/)",
  "repository": "https://github.com/blacknred/full-taskapp.git",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npx nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npx nest start",
    "start:dev": "npx nest start --watch",
    "start:debug": "npx nest start --debug --watch",
    "start:prod": "node dist/apps/auth-service/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "kompose": "kompose convert -o kubernetes/deployment.yaml",
    "docs": "npx compodoc --theme material --output docs -p tsconfig.json -s",
    "db-up": "npx mikro-orm migration:create && npx mikro-orm migration:up",
    "db-gen": "npx mikro-orm generate-entities --save --path=./libs/entities"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.17",
    "@grpc/proto-loader": "^0.7.7",


    "@nestjs/axios": "^1.0.1",
    "@nestjs/common": "^9.4.3",
    "@nestjs/config": "^1.2.1",
    "@nestjs/core": "^10.0.5",
    "@nestjs/cqrs": "8.0.5",
    "@nestjs/microservices": "^8.4.7",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "type-fest": "^1.4.0",
    "uuid": "^9.0.0",


    "@nestjs/mapped-types": "^2.0.2",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.14.0",
    "joi": "^17.9.2",


    "@nestjs/jwt": "^8.0.1",
    "@nestjs/passport": "^8.2.2",
    "cookie-parser": "^1.4.6",
    "bcryptjs": "^2.4.3",
    "qrcode": "^1.5.3",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "passport-custom": "^1.1.1",
    "passport-github": "^1.1.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    

    "@mikro-orm/core": "^5.7.12",
    "@mikro-orm/entity-generator": "^5.7.12",
    "@mikro-orm/nestjs": "^4.3.1",
    "@mikro-orm/postgresql": "^5.7.12",
    "pg": "^8.11.1",
    "nestjs-redis": "^1.3.3",
    "node-eventstore-client": "^21.10.1",
    "nestjs-amqp": "^0.2.2",
    "amqp-connection-manager": "^3.9.0",
    "amqplib": "^0.8.0",


    "pino": "^8.14.1",
    "pino-pretty": "^3.6.1",
    "nestjs-pino": "^1.4.0",
    "prom-client": "^14.2.0",
    "@godaddy/terminus": "^4.12.1",
    "@dollarsign/nestjs-jaeger-tracing": "^1.0.8",
    "@nestjs/terminus": "^8.1.1",


    "@nest-modules/mailer": "^1.3.22",
    "nodemailer": "^6.9.3",
    "twilio": "^3.84.1",
    "web-push": "^3.6.3"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.21",
    
    "@mikro-orm/cli": "^5.7.12",
    "@mikro-orm/migrations": "^5.7.12",
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.7",
    "@types/amqplib": "^0.8.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.17",
    "@types/jest": "27.4.1",
    "@types/node": "^16.18.38",
    "@types/nodemailer": "^6.4.8",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@types/web-push": "^3.3.2",
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "lint-staged": "^13.2.3",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^27.1.5",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.14.2",
    "typescript": "^4.9.5"
  },
  "packageManager": "pnpm@7.0.0",
  "jest": {
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    },
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "modulePaths": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "@taskapp/core/(.*)": "<rootDir>/libs/core/src/$1",
      "@taskapp/core": "<rootDir>/libs/core/src",
      "@taskapp/eventstore/(.*)": "<rootDir>/libs/eventstore/src/$1",
      "@taskapp/eventstore": "<rootDir>/libs/eventstore/src",
      "@taskapp/shared/(.*)": "<rootDir>/libs/shared/src/$1",
      "@taskapp/shared": "<rootDir>/libs/shared/src"
    }
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  },
  "prettier": {
    "trailingComma": "all",
    "singleQuote": true
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
      "project": "tsconfig.json",
      "sourceType": "module",
      "tsconfigRootDir": "."
    },
    "plugins": [
      "@typescript-eslint/eslint-plugin"
    ],
    "extends": [
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "ignorePatterns": [
      ".eslintrc.js"
    ],
    "rules": {
      "@typescript-eslint/interface-name-prefix": "off",
      "@typescript-eslint/explicit-function-return-type": "off",
      "@typescript-eslint/explicit-module-boundary-types": "off",
      "@typescript-eslint/no-explicit-any": "off"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(ts|tsx)": [
      "pnpm format",
      "pnpm lint"
    ]
  },
  "pnpm": {
    "overrides": {
      "class-validator@<0.14.0": ">=0.14.0",
      "passport@<0.6.0": ">=0.6.0",
      "jsonwebtoken@<=8.5.1": ">=9.0.0",
      "passport-oauth2@<1.6.1": ">=1.6.1",
      "pug@<3.0.1": ">=3.0.1",
      "pac-resolver@<5.0.0": ">=5.0.0",
      "degenerator@<3.0.1": ">=3.0.1",
      "jsonwebtoken@<9.0.0": ">=9.0.0",
      "@nestjs/core@<9.0.5": ">=9.0.5",
      "axios@<0.21.2": ">=0.21.2",
      "webpack@>=5.0.0 <5.76.0": ">=5.76.0",
      "yaml@>=2.0.0-5 <2.2.2": ">=2.2.2",
      "nth-check@<2.0.1": ">=2.0.1",
      "semver@<7.5.2": ">=7.5.2",
      "http-cache-semantics@<4.1.1": ">=4.1.1"
    }
  }
}
