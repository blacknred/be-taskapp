{
  "name": "@taskapp/service-core",
  "version": "0.0.1",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "description": "Taskapp microservice core library",
  "private": true,
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postinstall": "nest build",
